[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "upstream"]
	url = https://github.com/Kotlin/kotlin-koans
	fetch = +refs/heads/*:refs/remotes/upstream/*
[remote "origin"]
	url = git@github.com:cswl/coding_kotlin-koans.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = upstream
	merge = refs/heads/master
[branch "resolutions"]
	remote = upstream
	merge = refs/heads/resolutions
[branch "solutions"]
	remote = origin
	merge = refs/heads/solutions

